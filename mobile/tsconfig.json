{
    "compilerOptions": {
        "outDir": "./build/",
        "rootDir": "./src",
        "sourceMap": true,
        "module": "commonjs",
        "target": "es6",
        "jsx": "react-native",
        "noImplicitAny": true,
        "noUnusedLocals": true,
        "strict": true,
        "skipLibCheck": true
    },
    "include": [
        "./src/**/*"
    ]
}